(fn myfun [arg1 arg2]:
  (print :hello :world)
  (print arg1 arg2)
  (print :end))

; 上述定义等价于如下：
(let myfun
     (fn [arg1 arg2]:
       (print :hello :world)
       (print arg1 arg2)
       (print :end)))

